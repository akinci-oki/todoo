{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console": "warn",
        "quotes": ["warn", "double"],
        "semi": ["warn", "always"],
        "indent": ["warn", 4],
        "comma-dangle": ["warn", "always-multiline"],
        "space-before-function-paren": 0,
        "react/react-in-jsx-scope": 0,
        "multiline-ternary": 0
    }
}
